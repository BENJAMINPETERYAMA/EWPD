		 	<SelectSQLQuery name="retrieveQuestionDesc">
				select  ADQM_ADMN_QSTN_NBR, DTYP_DATA_TYPE_ID, 
						ADQM_ADMN_QSTN_DESC,QSTN_STTS_CD, QSTN_VRSN_NBR,
						LST_CHG_USR,LST_CHG_TMS,CREATD_USER_ID,CREATD_TM_STMP
				from 	ADQM_ADMN_QSTN_MSTR 
				where 	UPPER(ADQM_ADMN_QSTN_DESC) = UPPER({0})	and QSTN_VRSN_NBR = 
						(select max(QSTN_VRSN_NBR)	from ADQM_ADMN_QSTN_MSTR where UPPER(ADQM_ADMN_QSTN_DESC) = UPPER({0}))		
			</SelectSQLQuery>	
			<SelectSQLQuery name="retrieveSPSDesc">
				select a.cd_desc_txt, dbms_random.random
				 from item a,ADQM_ADMN_QSTN_MSTR b  
				where a.ctlg_id='1938' and b.ADQM_ADMN_QSTN_NBR = {0} 
				and a.prmry_cd =b.sps_id ORDER BY a.prmry_cd 	
			</SelectSQLQuery>
			<SelectSQLQuery name="selectTerm">
				select i.PRMRY_CD,i.CD_DESC_TXT, dbms_random.random
						from item i,catalog c 
						where c.CTLG_ID = i.CTLG_ID and upper(c.CTLG_NAME) ='TERM' and i.prmry_cd {0} order by i.PRMRY_CD asc   
			</SelectSQLQuery>
			<SelectSQLQuery name="selectQualifier">
