			<SelectSQLQuery name="questionSearch" dynamic="true">				
				<STATIC>
					<![CDATA[
					select DISTINCT A.DTYP_DATA_TYPE_ID,A.ADQM_ADMN_QSTN_DESC,A.ADQM_ADMN_QSTN_NBR,B.DTYP_DATA_TYPE_LGND, 
						   A.QSTN_STTS_CD, A.QSTN_VRSN_NBR, A.PRVDR_ARNGMNT, A.SPS_ID, D.cd_desc_txt,A.is_admn_mthd_mapped
					from   ADQM_ADMN_QSTN_MSTR A , DTYP_DATA_TYPE B,QSTN_DOMN_ASSN C, (select cd_desc_txt,prmry_cd from item where ctlg_id='1938' and prmry_cd {3} ORDER BY prmry_cd ) D
					where  1=1
					]]>
				</STATIC>
				<DYNAMIC>
					<![CDATA[
							AND UPPER(A.ADQM_ADMN_QSTN_DESC) like UPPER({1}) escape '`'
					]]>
				</DYNAMIC> 
				<DYNAMIC>
					<![CDATA[
							AND B.DTYP_DATA_TYPE_ID {0} 
					]]>
				</DYNAMIC> 
				<DYNAMIC>
					<![CDATA[
							AND C.DOMN_CD {2} 
					]]>
				</DYNAMIC>
				<DYNAMIC>
					<![CDATA[
							AND UPPER(A.SPS_ID) {3} 
					]]>
				</DYNAMIC>
				<STATIC>
					<![CDATA[
						AND A.DTYP_DATA_TYPE_ID = B.DTYP_DATA_TYPE_ID
						AND A.ADQM_ADMN_QSTN_NBR = C.ADQM_ADMN_QSTN_NBR
						AND A.SPS_ID= D.prmry_cd (+)
						AND A.QSTN_VRSN_NBR = (select max(C.QSTN_VRSN_NBR)	from ADQM_ADMN_QSTN_MSTR C where UPPER(C.ADQM_ADMN_QSTN_DESC) = UPPER(A.ADQM_ADMN_QSTN_DESC)
						)
						ORDER BY UPPER(A.ADQM_ADMN_QSTN_DESC)
					]]>
				</STATIC>
			</SelectSQLQuery>
			<SelectSQLQuery name="locateQuestions">	 
					<![CDATA[
					SELECT * FROM (
					SELECT DISTINCT (ADQM_QSTN_MSTR_PARNT_SYS_ID), ADQM_ADMN_QSTN_DESC
			           FROM ADQM_ADMN_QSTN_MSTR QM, QSTN_DOMN_ASSN QDA
			          WHERE QM.QSTN_STTS_CD != 'MARKED_FOR_DELETION'
			            AND QM.ADQM_QSTN_MSTR_PARNT_SYS_ID NOT IN (
			                   SELECT DISTINCT ENTY_SYS_ID
			                              FROM NOTE_DOMN_ASSN NDA, NMST_NOTE_MSTR NMST
			                             WHERE ENTY_TYPE = 'AVAILQUESTION'
			                               AND BNFT_DEFN_SYS_ID IS NULL
			                               AND NDA.NOTE_ID = {1}
			                               AND NDA.NOTE_ID = NMST.NOTE_ID
			                               AND NMST.NOTE_STTS_CD != 'MARKED_FOR_DELETION'
			                               AND NDA.NOTE_VRSN_NBR = NMST.NOTE_VRSN_NBR)
			            AND QM.ADQM_ADMN_QSTN_NBR = QDA.ADQM_ADMN_QSTN_NBR
			            AND QDA.DOMN_CD IN ('INFO-CC','INFO-SS', 'C-CNFG', 'C-MNUL')
			            AND QM.ADQM_ADMN_QSTN_DESC LIKE {0} ESCAPE '`'
			       ORDER BY QM.ADQM_ADMN_QSTN_DESC ASC) WHERE ROWNUM <=51	]]>				
			</SelectSQLQuery> 
				
