<metaData>
	<!--  Two sample metaobject definitions. MetaObject definitions for the application
		  have to be defined in the following format for the Business Object and specify the
		  file path in the metaobject bean in object-context.xml. 
		  
		  The status-transition-universe tag defined below have to be copied as it is into
		  the metaobject definition file directly under the metaData tag -->
	<metaObject name="com.wellpoint.wpd.common.bo.BO1" type="BO1" module="BO1">
		<checkoutDuration value="20" />
		<builderClass name="com.xyz.package.BO1Builder" />

		<locate>
			<criteria name="com.wellpoint.wpd.common.bo.BO1LocateCriteria"/>
			<criteria name="com.wellpoint.wpd.common.bo.SubBO3LocateCriteria"/>
			<criteria name="com.wellpoint.wpd.common.bo.SubBO4LocateCriteria"/>
		</locate>
		<informationElement name="boKey1" type="String" attrType="key" />
		<informationElement name="boKey2" type="String" attrType="key" />
		<informationElement name="boKey3" type="List" attrType="key" />
		<status-transitions>
			<transition name="TRANSITION_BUILDING_TO_CHECKED_IN"/>
			<transition name="TRANSITION_CHECKED_IN_TO_SCHEDULED_FOR_TEST"/> 
			<transition name="TRANSITION_SCHEDULED_FOR_TEST_TO_TEST_FAILED"/>
			<transition name="TRANSITION_SCHEDULED_FOR_TEST_TO_TEST_SUCCESSFUL"/>
			<transition name="TRANSITION_TEST_SUCCESSFUL_TO_SCHEDULED_FOR_APPROVAL"/>
			<transition name="TRANSITION_SCHEDULED_FOR_APPROVAL_TO_APPROVED"/> 
			<transition name="TRANSITION_SCHEDULED_FOR_APPROVAL_TO_REJECTED"/>
			<transition name="TRANSITION_REJECTED_TO_CHECKED_OUT"/>

		</status-transitions>
		<security>
			<activity name="create" taskName="CREATE"/>
			<activity name="edit" taskName="EDIT"/>
			<activity name="delete" taskName="DELETE"/>
			<activity name="retrieve" taskName="VIEW"/>
			<activity name="copy" taskName="COPY"/>
		</security>
	</metaObject>
