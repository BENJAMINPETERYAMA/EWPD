				                          FROM blvl_bnft_lvl
				                         WHERE bnft_defn_sys_id IN (
				                                                    SELECT bnft_defn_sys_id
				                                                      FROM bdm_bnft_defn_mstr
				                                                     WHERE bnft_sys_id = {0} 	))
