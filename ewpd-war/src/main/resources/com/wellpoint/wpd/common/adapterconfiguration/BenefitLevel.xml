		 	<SelectSQLQuery name="benefitLevelSearch">
					select A.BLVL_BNFT_LVL_ID,A.BNFT_DEFN_SYS_ID,b.CD_DESC_TXT, c.CD_DESC_TXT,
					A.BLVL_BNFT_LVL_DESC,d.CD_DESC_TXT,A.BNFT_LVL_SQNC,A.BLVL_BNFT_LVL_TERM,A.BLVL_BNFT_LVL_QLFR,A.BNFT_LVL_EXTRNL_RFRNC_ID
					from BLVL_BNFT_LVL A,ITEM b,ITEM c,ITEM d 
					where A.BNFT_DEFN_SYS_ID = {0}
					and A.BLVL_BNFT_LVL_TERM like {1}
					and (A.BLVL_BNFT_LVL_TERM = b.PRMRY_CD(+) AND b.CTLG_ID(+) = 1934)
					and (A.BLVL_BNFT_LVL_QLFR = c.PRMRY_CD(+) AND c.CTLG_ID(+) = 1935)
					and (A.BNFT_LVL_EXTRNL_RFRNC_ID = d.PRMRY_CD(+) AND d.CTLG_ID(+) = 1938) order by A.BNFT_LVL_SQNC
			</SelectSQLQuery>
			<SelectSQLQuery name="benefitTermSearch">
